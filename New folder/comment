 for y in v.first_ext[x]:
            #strips the unicode from all the entries 
            v.first_ext[x][y] = v.first_ext[x][y].replace('\u274f', '')
            #strips any remaining spaces
            v.first_ext[x][y] = v.first_ext[x][y].replace(' ', '')
